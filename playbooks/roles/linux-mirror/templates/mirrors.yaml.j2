# Automatically generated file by kdevops {{ kdevops_version }}
#
# Processed by start-mirroring.py.
#
# Keep in alphabetical order using the short_name as index, this order is
# only important so we know where to add new entries. Mirror references are
# dealt with through a second iteration by start-mirroring.py on this list.
# Only references with two levels are supported.
mirrors:
  - short_name: "blktests"
    url: "{{ mirror_blktests_url }}"
    target: "blktests.git"
  - short_name: "blktrace"
    url: "{{ defaults_blktrace_git }}"
    target: "blktrace.git"
  - short_name: "dbench"
    url: "{{ defaults_dbench_git }}"
    target: "dbench.git"
  - short_name: "fstests"
    url: "{{ mirror_fstests_url }}"
    target: "fstests.git"
  - short_name: "git"
    url: "{{ defaults_git_git }}"
    target: "git.git"
  - short_name: "kdevops-linus"
    url: "{{ mirror_kdevops_linus_url }}"
    target: "kdevops-linus.git"
    refresh: "360m"
    refresh_on_boot: "10m"
    reference: "linux.git"
  - short_name: "kdevops"
    url: "{{ mirror_kdevops_url }}"
    target: "kdevops.git"
    refresh: "5m"
    refresh_on_boot: "10m"
  - short_name: "kdevops-results-archive"
    url: "{{ defaults_kdevops_results_archive_git }}"
    target: "kdevops-results-archive.git"
  - short_name: "kdevops/fstests"
    url: "{{ mirror_kdevops_fstests_url }}"
    target: "kdevops/fstests.git"
    reference: "fstests.git"
  - short_name: "linux"
    url: "{{ mirror_torvalds_url }}"
    target: "linux.git"
    refresh: "5m"
    refresh_on_boot: "10m"
  - short_name: "linux-next"
    url: "{{ mirror_linux_next_url }}"
    target: "linux-next.git"
    reference: "linux.git"
    refresh: "360m"
    refresh_on_boot: "10m"
  - short_name: "ltp"
    url: "{{ defaults_ltp_git }}"
    target: "ltp.git"
  - short_name: "nfstest"
    url: "{{ defaults_nfstest_git }}"
    target: "nfstest.git"
  - short_name: "mcgrof-linus"
    url: "{{ mirror_mcgrof_linus_url }}"
    target: "mcgrof-linus.git"
    refresh: "360m"
    refresh_on_boot: "10m"
