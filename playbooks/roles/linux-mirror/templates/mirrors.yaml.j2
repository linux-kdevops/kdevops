    refresh_on_boot: "10m"
    reference: "linux-next.git"
  - short_name: "modules"
    url: "{{ mirror_modules_url }}"
    target: "linux-modules.git"
    reference: "linux.git"
  - short_name: "qemu"
    target: "qemu.git"
    url: "{{ mirror_qemu_jic23_url }}"
  - short_name: "qemu-jic23"
    target: "qemu-jic23.git"
    url: "{{ mirror_qemu_url }}"
    refresh_on_boot: "10m"
    reference: "linux-next.git"
  - short_name: "stable"
    url: "{{ mirror_stable_url }}"
